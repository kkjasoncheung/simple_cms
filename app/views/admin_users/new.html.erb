<% @page_title="New Admin User" %>


<%= render('shared/back') %>


<div class="admin_users new">
	<h2>Create New Admin User </h2>

	<%= form_for(:admin_user, :url=>{:action=>'create'}) do |f| %>

		<%= render(:partial=>'form', :locals=>{:f => f }) %>
		
		<div class="form-buttons">
			<%= submit_tag('Create User') %>
		</div>

	<% end %>
	
</div>